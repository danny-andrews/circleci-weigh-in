{
  "name": "thresh",
  "version": "0.2.5",
  "description": "A CircleCI integration script for tracking file size changes across deploys.",
  "keywords": [
    "assets",
    "circleci",
    "github",
    "gulp",
    "rollup",
    "size"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danny-andrews/thresh.git"
  },
  "license": "MIT",
  "author": "Danny Andrews",
  "files": [
    "lib/",
    "thresh"
  ],
  "main": "index.js",
  "bin": {
    "thresh": "./thresh"
  },
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "ci": "yarn lint && yarn test:coverage && yarn stats",
    "lint": "eslint . && sort-package-json",
    "stats": "echo 'LOC IN PROJECT...' && find src -name '*.js' -not -path '**/__tests__/*' | xargs wc -l",
    "test": "ava --verbose",
    "test:coverage": "nyc yarn test"
  },
  "dependencies": {
    "ajv": "^5.2.2",
    "command-line-args": "^4.0.6",
    "filesize": "^3.5.10",
    "flat-file-db": "^1.0.0",
    "humps": "^2.0.1",
    "mkdirp": "^0.5.1",
    "monet": "^0.8.10",
    "ncurry": "^1.0.0",
    "node-fetch": "^1.7.1",
    "ramda": "^0.24.1",
    "sprintf-js": "^1.1.1",
    "toml": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@danny.andrews/eslint-config": "^9.0.0",
    "ava": "1.0.0-beta.8",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "expect": "^1.20.2",
    "nyc": "^11.1.0",
    "sort-package-json": "^1.6.0"
  }
}
